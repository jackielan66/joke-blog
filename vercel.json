{
    "headers": [
        {
            "source": "/_next/static/*",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/_next/data/*",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=86400, stale-while-revalidate=86400"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/old-path",
            "destination": "/new-path"
        }
    ],
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node",
            "config": {
                "buildCommand": "prisma generate && next build"
            }
        }
    ]
}